{
    "version": 2,
    "builds": [
        { "src": "./backend/index.js", "use": "@vercel/node" },
        { "src": "./frontend", "use": "@vercel/vite" }
    ],
    "routes": [
        { "src": "/api/(.*)", "dest": "/backend/index.js/$1" },
        { "src": "/(.*)", "dest": "/frontend/$1" }
    ]
}
